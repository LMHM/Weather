{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ]
  },
  "Settings": {
    "Period": 60,
    "StartSecond": 5,
    "InputPath": "C:\\Testdata\\",
    "OutputPath": "C:\\Testdata\\"
  }
}
